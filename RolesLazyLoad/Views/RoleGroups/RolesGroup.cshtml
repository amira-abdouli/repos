
@model IEnumerable<RolesLazyLoad.Models.RoleGroupsJoinRoles>


<table class="table">
    <tr>

        <th>
            @Html.DisplayNameFor(model => model.Roles.Name)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>

            <td>
                @Html.DisplayFor(modelItem => item.Roles.Name)
            </td>
            <td>

                @Html.ActionLink("Delete", "RolesGroupDelete", new { RoleGroupsID = item.RoleGroupsID, RolesID = item.RolesID })
            </td>
        </tr>
    }

</table>
